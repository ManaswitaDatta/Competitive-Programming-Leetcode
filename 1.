class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer, Integer> map = new HashMap<>();
        int n = nums.length;
        for(int i = 0; i < n; i++){
            int k = target - nums[i];
            if(map.containsKey(k))
                return new int[]{map.get(k), i};
            else map.put(nums[i], i);
        }
        return new int[2];
    }
}
